--------------------------------------------------------------------------------
--
--=== Includes
--
--------------------------------------------------------------------------------
include japi.ew
--------------------------------------------------------------------------------
--
--=== Constants
--
--------------------------------------------------------------------------------
--------------------------------------------------------------------------------
--
--=== Types
--
--------------------------------------------------------------------------------
--------------------------------------------------------------------------------
--
--=== Variables
--
--------------------------------------------------------------------------------
String Failed Failed = "*** JAPI2 couldn't start ***"
String NotFound NotFound = "*** JAPI2 Library not found ***"
--------------------------------------------------------------------------------
--
--=== Routines
--
--------------------------------------------------------------------------------
procedure go(Japi j)
    JObject event
    JFrame jframe
    if j.start() then 
        jframe = JFrame("Frame Demo")
        jframe.show()
        event = 0
        while event != jframe do
            event = event.next()
        end while
        j.quit()
    else Failed.show(LF)
    end if
end procedure
--------------------------------------------------------------------------------
procedure main()
    Japi japi2
    japi2 = Japi("\\dll\\32\\japi2.dll")
    if japi2 then go(japi2)
    else NotFound.show(LF)
    end if
end procedure
--------------------------------------------------------------------------------
--
--=== Classes
--
--------------------------------------------------------------------------------
--------------------------------------------------------------------------------
-- Execution
--------------------------------------------------------------------------------
main()
